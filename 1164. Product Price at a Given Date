with p1 as (
    select *, row_number() over(partition by product_id order by change_date desc) as rn
    from products 
    where change_date <= '2019-08-16'
)
, p2 as (
    select distinct product_id
    from products
    where product_id not in (select product_id from p1)
)
select product_id, new_price as price from p1 where rn = 1
union all 
select product_id, 10 as price from p2
